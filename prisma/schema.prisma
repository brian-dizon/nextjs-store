generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Product {
  id  String  @id @default(uuid())
  name  String
  company String
  description String
  featured Boolean
  image String
  price Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  clerkId String
}
 